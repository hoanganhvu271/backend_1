doctype html
html
  head
    title #{titlePage}
    link(rel="stylesheet",  href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css")
    link(rel="stylesheet",  href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css")
    link(rel="stylesheet",  href="/css/index.css")
    link(rel="stylesheet", href="/css/new.css")
    link(rel="stylesheet", href="/css/edit-test.css")
    script(src="/js/authen.js")
  body.app
    include ../partials/header.pug
    .page-container
      include ../partials/nav-header.pug
      main(class='main-content bgc-grey-100')
        .main-container
          .alert.alert-danger.alert-dismissible.fade.show#myAlert(role="alert")
            button.close(data-dismiss="alert" aria-label="Close")
              span(aria-hidden="true") &times;
            span#alertContent
          block main


    script(src="/js/upload-test.js")
    script(src="/js/permission.js")
    script(src="/js/delete-test.js")
    script(src="/js/pagination.js")
    script(src="/js/result.js")
    script(src="/js/student-account.js")
